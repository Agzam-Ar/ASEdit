  "rules": [
    { "token": "keyword", "foreground": "CC6C1D", "note": "CC6C1D", "fontStyle": "bold"  },
    { "token": "function", "foreground": "96EC3F", "note": "CC6C1D"},
    { "token": "wrong", "foreground": "ff0000" },
    { "token": "operator", "foreground": "ffffff" },
    { "token": "string", "foreground": "72CA9B", "note": "green" },
    { "token": "char", "foreground": "72CA9B", "note": "green" },
    { "token": "blocks", "foreground": "13C9BA", "note": "turquoise" },

    { "token": "", "foreground": "F6F7F9" },
    { "token": "invalid", "foreground": "ff0000", "background": "ff0000", "fontStyle": "underline" },

    { "token": "emphasis", "fontStyle": "italic" },
    { "token": "strong", "fontStyle": "bold" },
    { "token": "identifier", "foreground": "F3EC79", "note": "orange" },
    { "token": "variable", "foreground": "EF596F", "note": "red" },
    { "token": "variable.function", "foreground": "96EC3F", "note": "blue" },
    { "token": "constant", "foreground": "F0B726", "note": "gold" },
    { "token": "constant.function", "foreground": "BDADFF", "note": "indigo" },
    { "token": "number", "foreground": "6897BB", "note": "gold" },
    { "token": "annotation", "foreground": "13C9BA", "note": "turquoise" },
    { "token": "type", "foreground": "EC9A3C", "note": "orange" },
    { "token": "delimiter", "foreground": "F6F7F9", "note": "white" },
    { "token": "tag", "foreground": "43BF4D", "note": "forest" },
    { "token": "meta", "foreground": "61AFEF", "note": "blue" },
    { "token": "key", "foreground": "EF596F", "note": "red" },
    { "token": "operators", "foreground": "3FA6DA", "note": "cerulean" },
    { "token": "attribute", "foreground": "EC9A3C", "note": "orange" },
    { "token": "comment", "foreground": "#8F99A8", "fontStyle": "italic" }

    
  ],
  "encodedTokensColors": []
}